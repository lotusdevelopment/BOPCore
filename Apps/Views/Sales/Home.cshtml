@using Apps.Controllers
@using Apps.App_Start

<link href="@Url.Content("~/Content/vendor/plugins/nvd3/build/nv.d3.min.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/vendor/plugins/jquery-circliful/css/jquery.circliful.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/vendor/plugins/jquery-circliful/js/jquery.circliful.min.js")"></script>
